import math as m
print("Format of quadratic equation, ax**2+bx+c")
a=int(input("enter the value of a:"))
b=int(input("enter the value of b:"))
c=int(input("enter the value of :"))

d=b*b-4*a*c

if d>0:
    print("roots of equation are real")
    r1=(-b+m.sqrt(d))/(2*a)
    r2=(-b-m.sqrt(d))/(2*a)
    print(r1,r2)

elif d==0:
    print("roots are equal")
    r1=-b/(2*a)
    r1=r2
    print(r1,r2)

else:
    print("roots do not exist i.e they are imaginary")
